<div class="card">
  <div class="card-body card-padding">
    <table class="table">
        <thead>
            <tr>
                <th>#</th>
                <th>奖券唯一码</th>
                <th>奖品</th>
                <th>操作</th>
            </tr>
        </thead>
        <tbody>
        <% @coupons.each do |coupon| %>
            <tr>
                <th scope="row"><%= coupon.id %></th>
                <td><%= coupon.serial_number %></td>
                <td><%= coupon.ticket.name %></td>
                <td>
                    <%= link_to "修改", edit_admin_coupon_path(coupon) %>
                    <%= link_to '删除', admin_coupon_path(coupon), method: :delete %>
                </td>
            </tr>
        <% end %>
    </table>
  </div>
</div>