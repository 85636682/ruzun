<div class="row">
  <div class="col-lg-12">
    <h1 class="page-header">技术统计</h1>
  </div>
  <!-- /.col-lg-12 -->
</div>
<!-- /.row -->
<div class="row">
  <div class="col-lg-12">
    <div class="panel panel-default">
      <div class="panel-heading">
        <%= @played.home_team.name %> vs <%= @played.guest_team.name %> : <%= @techstat.user.name %>
      </div>
      <!-- /.panel-heading -->
      <div class="panel-body">
        <%= form_for [:admin, @game, @played, @techstat] do |f| %>
          <div class="form-group">
            <label>主队：</label>
            <%= f.text_field :pts, class: "form-control" %>
          </div>
          <div class="form-group">
            <label>主队：</label>
            <%= f.text_field :ast, class: "form-control" %>
          </div>
          <div class="form-group">
            <label>主队：</label>
            <%= f.text_field :stl, class: "form-control" %>
          </div>
          <div class="form-group">
            <label>主队：</label>
            <%= f.text_field :blk, class: "form-control" %>
          </div>
          <div class="form-group">
            <label>主队：</label>
            <%= f.text_field :off, class: "form-control" %>
          </div>
          <div class="form-group">
            <label>主队：</label>
            <%= f.text_field :reb, class: "form-control" %>
          </div>
          <div class="form-group">
            <label>主队：</label>
            <%= f.text_field :pm3, class: "form-control" %>
          </div>
          <div class="form-group">
            <label>主队：</label>
            <%= f.text_field :pm3_a, class: "form-control" %>
          </div>
          <div class="form-group">
            <label>主队：</label>
            <%= f.text_field :ftm, class: "form-control" %>
          </div>
          <div class="form-group">
            <label>主队：</label>
            <%= f.text_field :ftm_a, class: "form-control" %>
          </div>
          <div class="form-group">
            <label>主队：</label>
            <%= f.text_field :fgm, class: "form-control" %>
          </div>
          <div class="form-group">
            <label>主队：</label>
            <%= f.text_field :fgm_a, class: "form-control" %>
          </div>
          <%= f.submit "保存", class: "btn btn-primary" %>
        <% end %>
      </div>
    </div>
  </div>
</div>
