<div class="mdui-container-fluid">
    <div class="mdui-row">
        <div class="mdui-col-xs-6">
            <%= image_tag("bluefairy2.jpg", width: 80, height: 80) %>
        </div>
        <div class="mdui-col-xs-6">
            <p class="mdui-valign mdui-center">一张图读懂</p>
            <p class="mdui-valign mdui-center">蓝精灵升级俱乐部众筹</p>
        </div>
    </div>
    <div class="mdui-divider"></div>
    <% @crowdfundings.each do |crowdfunding| %>
    <div class="mdui-row">
        <div class="mdui-col-xs-3">
            <p class="mdui-valign mdui-center"></p><%= crowdfunding.fee %>
        </div>
        <div class="mdui-col-xs-9">
            <p class="mdui-valign mdui-center"><%= crowdfunding.desc %></p>
        </div>
    </div>
    <div class="mdui-divider"></div>
    <% end %>
</div>