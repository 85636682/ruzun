<div class="mdui-container">
  <ul class="mdui-list">
  <% @students.each do |student| %>
    <a href="<%= m_student_path(student) %>">
      <li class="mdui-list-item mdui-ripple">
        <i class="mdui-list-item-avatar mdui-icon material-icons">&#xe2c7;</i>
        <div class="mdui-list-item-content">
          <div class="mdui-list-item-title"><%= student.name %></div>
          <div class="mdui-list-item-text"><%= student.phone %></div>
        </div>
        <i class="mdui-list-item-icon mdui-icon material-icons">&#xe88e;</i>
      </li>
    </a>
  <% end %>
  </ul>
</div>